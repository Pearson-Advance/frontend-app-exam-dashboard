{
  "name": "@edx/frontend-app-exam-dashboard",
  "version": "0.1.0",
  "description": "Frontend application template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pearson-Advance/frontend-app-exam-dashboard"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "fedx-scripts formatjs extract",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "start:with-theme": "paragon install-theme && npm start && npm install",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edunext contact@edunext.co",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Pearson-Advance/frontend-app-exam-dashboard#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Pearson-Advance/frontend-app-exam-dashboard/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@openedx/brand-openedx@^1.2.2",
    "@edx/frontend-component-header": "^4.0.0",
    "@edx/frontend-platform": "^4.5.1",
    "@edx/paragon": "^20.45.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.2",
    "core-js": "^3.25.5",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-paragon-topaz": "^1.38.0",
    "react-redux": "7.2.9",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "redux": "4.2.1",
    "regenerator-runtime": "0.14.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "^12.8.57",
    "@edx/reactifex": "^2.1.1",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "^28.1.3",
    "webpack-cli": "4.10.0"
  }
}
