{
  "name": "@edx/frontend-app-exam-dashboard",
  "version": "0.1.0",
  "description": "Frontend app exam dashboard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pearson-Advance/frontend-app-exam-dashboard.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "eduNEXT contact@edunext.co",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Pearson-Advance/frontend-app-exam-dashboard#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Pearson-Advance/frontend-app-exam-dashboard/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.2.0",
    "@edx/frontend-component-footer": "12.0.0",
    "@edx/frontend-component-header": "4.2.3",
    "@edx/frontend-platform": "4.5.1",
    "@edx/paragon": "^20.45.0",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.1",
    "core-js": "3.31.0",
    "prop-types": "15.8.1",
    "provinces-ca": "1.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "^5.25.1",
    "react-paragon-topaz": "1.39.0",
    "react-router": "5.3.3",
    "react-router-dom": "5.3.3",
    "regenerator-runtime": "0.13.11",
    "states-us": "1.1.1",
    "world-countries": "5.1.0"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "^12.8.57",
    "@edx/reactifex": "^2.1.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "26.6.3"
  }
}
